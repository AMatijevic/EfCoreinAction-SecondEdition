@model BookApp.Infrastructure.Books.EventHandlers.CheckFixCode.CheckFixInputDto

@{
    ViewData["Title"] = "CacheCheckFix";
}

<h1>CacheCheckFix</h1>

<h4>CheckFixInputDto</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="CacheCheckFix">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="LookingBack" class="control-label"></label>
                <input asp-for="LookingBack" class="form-control" />
                <span asp-validation-for="LookingBack" class="text-danger"></span>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="FixBadCacheValues" /> @Html.DisplayNameFor(model => model.FixBadCacheValues)
                </label>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

